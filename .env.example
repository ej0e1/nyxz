# Database (Railway PostgreSQL)
DATABASE_URL=postgresql://user:pass@host:5432/dbname

# NextAuth
NEXTAUTH_SECRET=your_random_secret_min_32_chars
NEXTAUTH_URL=http://localhost:3000

# qBittorrent
QBIT_BASE_URL=https://yourdomain.com/qbittorrent
QBIT_USER=your_username
QBIT_PASS=your_password
QBIT_JOBS_PATH=/path/to/downloads/jobs

# Wasabi S3 (My Files)
WASABI_ENDPOINT=https://s3.wasabisys.com
WASABI_REGION=us-east-1
WASABI_ACCESS_KEY=your_access_key
WASABI_SECRET_KEY=your_secret_key
WASABI_BUCKET=your_bucket_name

# Seed (optional): SEED_USER_EMAIL=demo@example.com
